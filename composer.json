{
    "name": "ademozmermer/transbrowser",
    "description": "Change language by browser for Laravel",
    "keywords": [
        "laravel",
        "transbrowser"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/ademozmermer/transbrowser",
    "support": {
        "issues": "https://github.com/ademozmermer/transbrowser/issues",
        "source": "https://github.com/ademozmermer/transbrowser"
    },
    "authors": [
        {
            "name": "Adem Ã–zmermer",
            "email": "ademozmermer27@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/support": "5.0.*|5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|^6.0",
        "illuminate/validation": "5.0.*|5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|^6.0",
        "illuminate/translation": "5.0.*|5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|^6.0"
    },
    "autoload": {
        "psr-4": {
            "AdemOzmermer\\TransBrowser\\": "./src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": "AdemOzmermer\\TransBrowser\\TransBrowserServiceProvider"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
